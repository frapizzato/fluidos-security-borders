<!-- FLUIDOS extension for border protection (POLITO)  -->

	<!-- Used to define the Private intents, i.e., those regarding communications internal to the VCluster and not requiring any authorization. -->
	<complexType name="PrivateIntents">
		<complexContent>
			<extension base="ITResource:Configuration">
				<sequence>
					<element maxOccurs="unbounded" minOccurs="0" name="configurationRule" type="ITResource:ConfigurationRule"> </element>
					<!-- 
					<element maxOccurs="unbounded" minOccurs="0" name="NetworkSlicingConditionParameters" type="ITResource:NetworkSlicingConditionParameters"> </element>
					<element maxOccurs="1" minOccurs="0" name="resolutionStrategy" type="ITResource:ResolutionStrategy"> </element>
					<element maxOccurs="1" minOccurs="1" name="Name" type="string"/>
					 -->
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	
	<!-- Used to define the Request intents, i.e., those regarding communications crossing the VCluster border and that MUST be authorized by hosting cluster. -->
	<complexType name="RequestIntents">
		<complexContent>
			<extension base="ITResource:Configuration">
				<sequence>
					<element maxOccurs="1" minOccurs="1" name="acceptMonitoring" type="boolean"/>
					<element maxOccurs="unbounded" minOccurs="0" name="configurationRule" type="ITResource:ConfigurationRule"> </element>
					<!-- 
					<element maxOccurs="unbounded" minOccurs="0" name="NetworkSlicingConditionParameters" type="ITResource:NetworkSlicingConditionParameters"> </element>
					<element maxOccurs="1" minOccurs="0" name="resolutionStrategy" type="ITResource:ResolutionStrategy"> </element>
					<element maxOccurs="1" minOccurs="1" name="Name" type="string"/>
					 -->
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	
	<!-- Used to define the Authorization intents, they can be of two kinds:
			1) "ForbiddenList" - express the communications that are explicitly denied if requested by hosted resources
			2) "MandatoryList" - express the communications that are requested to be supported by hosted resources (e.g., open port X on offloaded resources for monitoring)
	     all the other connections that are requested, and not forbidden, are allowed. -->
	<complexType name="AuthorizationIntents">
		<complexContent>
			<extension base="ITResource:Configuration">
				<sequence>
					<element maxOccurs="unbounded" minOccurs="0" name="forbiddenConnectionList" type="ITResource:ConfigurationRule"> </element>
					<element maxOccurs="unbounded" minOccurs="0" name="mandatoryConnectionList" type="ITResource:ConfigurationRule"> </element>
					<!-- 
					<element maxOccurs="unbounded" minOccurs="0" name="NetworkSlicingConditionParameters" type="ITResource:NetworkSlicingConditionParameters"> </element>
					<element maxOccurs="1" minOccurs="0" name="resolutionStrategy" type="ITResource:ResolutionStrategy"> </element>
					<element maxOccurs="1" minOccurs="1" name="Name" type="string"/>
					 -->
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	
	<!-- To express the DENY/ALLOW action of each intent.
		 NOTE: IntraVClusterConf. + InterVClusterConf must be all "ALLOW", whereas AuthorizationInt. can use both "ALLOW" and "DENY" 
	-->
	<complexType name="KubernetesNetworkFilteringAction">
		<complexContent>
			<extension base="ITResource:ConfigurationAction">
				<sequence>
					<element name="KubernetesNetworkFilteringActionType" type="string"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<simpleType name="KubernetesNetworkFilteringActionType">
		<restriction base="string">
			<enumeration value="ALLOW"/>
			<enumeration value="DENY"/>
		</restriction>
	</simpleType>
	
	<!-- To express the condition for each "ConfigurationRule" -->
	<complexType name="KubernetesNetworkFilteringCondition">
		<complexContent>
			<extension base="ITResource:ConfigurationCondition">
				<sequence>
					<element maxOccurs="1" minOccurs="1" name="source" type="ITResource:ResourceSelector"/>
					<element maxOccurs="1" minOccurs="1" name="sourcePort" type="string"/>
					<element maxOccurs="1" minOccurs="1" name="destination" type="ITResource:ResourceSelector"/>
					<element maxOccurs="1" minOccurs="1" name="destinationPort" type="string"/>
					<element maxOccurs="1" minOccurs="1" name="protocolType" type="ITResource:ProtocolType"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="ResourceSelector">
		<sequence> 
			<element maxOccurs="1" minOccurs="1" name="isHostCluster" type="boolean"/>
		</sequence>
	</complexType>

	<complexType name="PodNamespaceSelector">
		<complexContent>
			<extension base="ITResource:ResourceSelector">
				<sequence>
					<element maxOccurs="unbounded" minOccurs="1" name="pod" type="ITResource:KeyValue"/>
					<element maxOccurs="unbounded" minOccurs="1" name="namespace" type="ITResource:KeyValue"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	
	<complexType name="CIDRSelector">
		<complexContent>
			<extension base="ITResource:ResourceSelector">
				<sequence>
					<element maxOccurs="1" minOccurs="1" name="addressRange" type="string"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	
	<simpleType name="ProtocolType">
		<restriction base="string">
			<enumeration value="TCP"/>
			<enumeration value="UDP"/>
			<enumeration value="SCTP"/>
			<enumeration value="ALL"/>
		</restriction>
	</simpleType>
