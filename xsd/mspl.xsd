<schema xmlns="http://www.w3.org/2001/XMLSchema"
	xmlns:ITResource="http://modeliosoft/xsddesigner/a22bd60b-ee3d-425c-8618-beb6a854051a/ITResource.xsd"
	targetNamespace="http://modeliosoft/xsddesigner/a22bd60b-ee3d-425c-8618-beb6a854051a/ITResource.xsd"
	elementFormDefault="qualified"
	xmlns:IoTHoneyNet="http://www.example.org/IoTHoneynetSchema">

	<import namespace="http://www.example.org/IoTHoneynetSchema" schemaLocation="iot-honeynet.xsd"/>

	<element name="ITResource" type="ITResource:ITResourceType"/>
	<!-- ANASTACIA policy for orchestration -->
	<element name="ITResourceOrchestration" type="ITResource:ITResourceOrchestrationType"/>
	<complexType name="ITResourceOrchestrationType">
		<sequence>
			<element maxOccurs="unbounded" minOccurs="0" name="ITResource" type="ITResource:ITResourceType"/>
			<element maxOccurs="1" minOccurs="0" name="priority" type="integer"/>
			<element maxOccurs="1" minOccurs="0" name="dependencies" type="ITResource:Dependencies"/>
			<!-- INSPIRE-5G+ ZSM enforcement domains-->
			<element maxOccurs="1" minOccurs="0" name="orchestrationRequirements" type="ITResource:OrchestrationRequirements"/>
      <element maxOccurs="1" minOccurs="0" name="enforcementRequirements" type="ITResource:EnforcementRequirements"/>
		</sequence>
		<attribute name="id" type="string"/>
		<attribute name="tenantID" type="string"/>
		<attribute name="sliceID" type="string"/>
	</complexType>

	<complexType name="ITResourceType">
		<sequence>
			<element maxOccurs="1" minOccurs="1" name="configuration" type="ITResource:Configuration"/>
			<!-- ANASTACIA policy for orchestration extension -->
			<element maxOccurs="1" minOccurs="0" name="priority" type="integer"/>
			<element maxOccurs="1" minOccurs="0" name="dependencies" type="ITResource:Dependencies"/>
			<element maxOccurs="1" minOccurs="0" name="enablerCandidates" type="ITResource:EnablerCandidates"/>
			<!-- /ANASTACIA policy for orchestration extension -->
			<!-- INSPIRE-5G+ ZSM enforcement domains-->
			<element maxOccurs="1" minOccurs="0" name="orchestrationRequirements" type="ITResource:OrchestrationRequirements"/>
      <element maxOccurs="1" minOccurs="0" name="enforcementRequirements" type="ITResource:EnforcementRequirements"/>
		</sequence>
		<attribute name="id" type="string"/>
		<attribute name="orchestrationID" type="string"/>
    <attribute name="tenantID" type="string"/>
		<attribute name="sliceID" type="string"/>
	</complexType>
	
		<complexType name="OrchestrationRequirements">
		<sequence>
			<element name="latency" type="integer" minOccurs="0" maxOccurs="unbounded"/>
			<element name="algorithm" type="string" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	
		<complexType name="EnforcementRequirements">
		<sequence>
			<element name="enforcementDomains" type="ITResource:EnforcementDomains" minOccurs="1" maxOccurs="1"/>
		</sequence>
	</complexType>
	
			<complexType name="EnforcementDomains">
		<sequence>
			<element name="domainID" type="string" minOccurs="1"	maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	
	<complexType name="Dependencies">
		<sequence>
			<element name="dependency" type="ITResource:Dependency" minOccurs="1"
				maxOccurs="unbounded"/>
		</sequence>
	</complexType>

	<complexType name="Dependency"> </complexType>


	<complexType name="PolicyDependency">
		<complexContent>
			<extension base="ITResource:Dependency">
				<sequence>
					<element name="configurationCondition"
						type="ITResource:PolicyDependencyCondition"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="PolicyDependencyCondition">
		<complexContent>
			<extension base="ITResource:ConfigurationCondition">
				<sequence>
					<element name="policyID" type="string"/>
					<element name="status" type="string"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="EventDependency">
		<complexContent>
			<extension base="ITResource:Dependency">
				<sequence>
					<element name="eventID" type="string"/>
					<element name="configurationCondition" type="ITResource:ConfigurationCondition"
					/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="EnablerCandidates">
		<sequence>
			<element name="enabler" type="string" maxOccurs="unbounded"/>
		</sequence>
	</complexType>



	<!-- /ANASTACIA policy for orchestration extension -->

	<complexType name="Configuration">
		<sequence>
			<element maxOccurs="unbounded" minOccurs="1" name="capability" type="ITResource:Capability"/>
		</sequence>
	</complexType>

	<complexType name="Capability">
		<sequence>
			<element maxOccurs="1" minOccurs="1" name="Name" type="ITResource:CapabilityType"/>
		</sequence>
	</complexType>

	<complexType name="AuthorizationCapability">
		<complexContent>
			<extension base="ITResource:Capability"/>
		</complexContent>
	</complexType>

	<complexType name="ChannelAuthorizationCapability">
		<complexContent>
			<extension base="ITResource:AuthorizationCapability"/>
		</complexContent>
	</complexType>

	<complexType name="FilteringCapability">
		<complexContent>
			<extension base="ITResource:ChannelAuthorizationCapability">
				<sequence>
					<element maxOccurs="1" minOccurs="1" name="stateful" type="boolean"/>
					<element maxOccurs="1" minOccurs="1" name="applicationLayerFiltering" type="boolean"/>
					<element maxOccurs="1" minOccurs="1" name="httpFiltering" type="boolean"> </element>
					<element maxOccurs="1" minOccurs="1" name="contentInspection" type="boolean">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="TargetAuthzCapability">
		<complexContent>
			<extension base="ITResource:AuthorizationCapability"/>
		</complexContent>
	</complexType>

	<complexType name="AuthzEnforcementCapability">
		<complexContent>
			<extension base="ITResource:TargetAuthzCapability"/>
		</complexContent>
	</complexType>

	<complexType name="AuthzDecisionCapabiliy">
		<complexContent>
			<extension base="ITResource:TargetAuthzCapability"/>
		</complexContent>
	</complexType>

	<complexType name="AuthenticationCapability">
		<complexContent>
			<extension base="ITResource:Capability"/>
		</complexContent>
	</complexType>

	<complexType name="AuthcEnforcementCapability">
		<complexContent>
			<extension base="ITResource:AuthenticationCapability"/>
		</complexContent>
	</complexType>

	<complexType name="AuthcDecisionCapability">
		<complexContent>
			<extension base="ITResource:AuthenticationCapability">
				<sequence>
					<element maxOccurs="1" minOccurs="1" name="authenticationMethods" type="string">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="TrafficAnalysisCapability">
		<complexContent>
			<extension base="ITResource:Capability">
				<sequence>
					<element maxOccurs="1" minOccurs="1" name="supportOnlineTraficAnalysis" type="boolean"/>
					<element maxOccurs="1" minOccurs="1" name="supportOfflineTraficAnalysis" type="boolean"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="IDSCapability">
		<complexContent>
			<extension base="ITResource:TrafficAnalysisCapability"/>
		</complexContent>
	</complexType>

	<complexType name="IPSCapability">
		<complexContent>
			<extension base="ITResource:TrafficAnalysisCapability"/>
		</complexContent>
	</complexType>

	<complexType name="MalwaresAnalysisCapability">
		<complexContent>
			<extension base="ITResource:TrafficAnalysisCapability"/>
		</complexContent>
	</complexType>

	<complexType name="MaliciousFileAnalysisCapability">
		<complexContent>
			<extension base="ITResource:MalwaresAnalysisCapability">
				<sequence>
					<element maxOccurs="1" minOccurs="1" name="fileType" type="string"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>


	<complexType name="LawfulInterceptionCapability">
		<complexContent>
			<extension base="ITResource:TrafficAnalysisCapability">
				<sequence>
					<element maxOccurs="unbounded" minOccurs="1" name="trafficType" type="string"/>
					<element maxOccurs="1" minOccurs="1" name="Country" type="string"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="ResourceScannerCapability">
		<complexContent>
			<extension base="ITResource:Capability">
				<sequence>
					<element maxOccurs="1" minOccurs="1" name="resourceType" type="string"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="VulnerabilitiesScannerCapability">
		<complexContent>
			<extension base="ITResource:ResourceScannerCapability"/>
		</complexContent>
	</complexType>

	<complexType name="BotnetDetectorCapability">
		<complexContent>
			<extension base="ITResource:VulnerabilitiesScannerCapability"/>
		</complexContent>
	</complexType>

	<complexType name="LoggingCapability">
		<complexContent>
			<extension base="ITResource:ResourceScannerCapability"/>
		</complexContent>
	</complexType>

	<complexType name="TrafficRecordCapability">
		<complexContent>
			<extension base="ITResource:LoggingCapability"/>
		</complexContent>
	</complexType>

	<complexType name="DataProtectionCapability">
		<complexContent>
			<extension base="ITResource:Capability">
				<sequence>
					<element maxOccurs="1" minOccurs="1"
						name="supportsDataAuthenticationAndIntegrity" type="boolean"/>
					<element maxOccurs="1" minOccurs="1" name="supportsDigitalSignature"
						type="boolean"/>
					<element maxOccurs="1" minOccurs="1" name="supportsEncryption" type="boolean"> </element>
					<element maxOccurs="1" minOccurs="1" name="supportsKeyExchange" type="boolean"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="EncryptionCapability">
		<complexContent>
			<extension base="ITResource:DataProtectionCapability"/>
		</complexContent>
	</complexType>

	<complexType name="IdentityProtectionCapability">
		<complexContent>
			<extension base="ITResource:Capability"/>
		</complexContent>
	</complexType>

	<complexType name="AnonimizerCapability">
		<complexContent>
			<extension base="ITResource:IdentityProtectionCapability"/>
		</complexContent>
	</complexType>

	<complexType name="AddressTranslationCapability">
		<complexContent>
			<extension base="ITResource:Capability"/>
		</complexContent>
	</complexType>

	<complexType name="ForwardProxyCapabiity">
		<complexContent>
			<extension base="ITResource:AddressTranslationCapability"/>
		</complexContent>
	</complexType>


	<complexType name="NattingCapability">
		<complexContent>
			<extension base="ITResource:ForwardProxyCapabiity"/>
		</complexContent>
	</complexType>

	<complexType name="ReverseProxyCapabiity">
		<complexContent>
			<extension base="ITResource:AddressTranslationCapability"/>
		</complexContent>
	</complexType>

	<complexType name="URLRewritingCapability">
		<complexContent>
			<extension base="ITResource:ReverseProxyCapabiity"/>
		</complexContent>
	</complexType>

	<complexType name="RoutingCapability">
		<complexContent>
			<extension base="ITResource:Capability"/>
		</complexContent>
	</complexType>
	
	<!-- INSPIRE-5GPlus 5GServiceSecuritySlice-->
	
	<complexType name="FiveGSecuritySlice">
		<complexContent>
			<extension base="ITResource:Capability">
				<sequence>
					<element maxOccurs="1" minOccurs="1" name="sliceID" type="integer"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>	
	
		<complexType name="FiveGSecuredService">
		<complexContent>
			<extension base="ITResource:Capability"/>
		</complexContent>
	</complexType>
	

	<complexType name="RuleSetConfiguration">
		<complexContent>
			<extension base="ITResource:Configuration">
				<sequence>
					<element maxOccurs="1" minOccurs="0" name="defaultAction"
						type="ITResource:ConfigurationAction"/>
					<element maxOccurs="unbounded" minOccurs="0" name="configurationRule"
						type="ITResource:ConfigurationRule"> </element>
					<element maxOccurs="unbounded" minOccurs="0" name="NetworkSlicingConditionParameters"
						type="ITResource:NetworkSlicingConditionParameters"> </element>
					<element maxOccurs="1" minOccurs="0" name="resolutionStrategy"
						type="ITResource:ResolutionStrategy"> </element>
					<element maxOccurs="1" minOccurs="1" name="Name" type="string"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="ConfigurationAction"> </complexType>

	<complexType name="ConfigurationRule">
		<sequence>
			<element maxOccurs="1" minOccurs="1" name="configurationRuleAction"
				type="ITResource:ConfigurationAction"> </element>
			<element maxOccurs="1" minOccurs="1" name="configurationCondition"
				type="ITResource:ConfigurationCondition"> </element>
			<element maxOccurs="1" minOccurs="0" name="externalData" type="ITResource:ExternalData"> </element>
			<element maxOccurs="1" minOccurs="1" name="Name" type="string"> </element>
			<element maxOccurs="1" minOccurs="1" name="isCNF" type="boolean"> </element>
			<element name="HSPL" type="ITResource:HSPL" maxOccurs="unbounded" minOccurs="0"/>
		</sequence>


	</complexType>
	<complexType name="ConfigurationCondition">
		<sequence>
			<element maxOccurs="1" minOccurs="1" name="isCNF" type="boolean"/>
		</sequence>
	</complexType>


	<complexType name="ExternalData"> </complexType>

	<complexType name="Priority">
		<complexContent>
			<extension base="ITResource:ExternalData">
				<sequence>
					<element maxOccurs="1" minOccurs="1" name="value" type="integer"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="ResolutionStrategy">
		<sequence>
			<element maxOccurs="unbounded" minOccurs="0" name="externalData"
				type="ITResource:ExternalData"/>
		</sequence>
	</complexType>

	<complexType name="FMR">
		<complexContent>
			<extension base="ITResource:ResolutionStrategy"/>
		</complexContent>
	</complexType>

	<complexType name="DTP">
		<complexContent>
			<extension base="ITResource:ResolutionStrategy"/>
		</complexContent>
	</complexType>

	<complexType name="ATP">
		<complexContent>
			<extension base="ITResource:ResolutionStrategy"/>
		</complexContent>
	</complexType>

	<complexType name="MSTP">
		<complexContent>
			<extension base="ITResource:ResolutionStrategy"/>
		</complexContent>
	</complexType>

	<complexType name="LSTP">
		<complexContent>
			<extension base="ITResource:ResolutionStrategy"/>
		</complexContent>
	</complexType>

	<complexType name="ALL">
		<complexContent>
			<extension base="ITResource:ResolutionStrategy"/>
		</complexContent>
	</complexType>

	<!-- INSPIRE-5GPlus 5GServiceSecuritySlice Actions and condition-->

	<complexType name="FiveGSecuritySliceCondition">
		<complexContent>
			<extension base="ITResource:ConfigurationCondition">
				<sequence>
					<element maxOccurs="unbounded" minOccurs="1" name="securedService" type="ITResource:SecuredService"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	

    <complexType name="SecuredService">
				<sequence>
          <element maxOccurs="1" minOccurs="1" name="service" type="ITResource:Service"/>
          <element maxOccurs="1" minOccurs="0" name="securityRequirements" type="ITResource:SSLALink"/>
          <element maxOccurs="1" minOccurs="0" name="securityPolicies" type="ITResource:MSPLLink"/>
				</sequence>
	</complexType>	
	
	    <complexType name="Service">
				<sequence>
          <element maxOccurs="1" minOccurs="0" name="name" type="string"/>
          <element maxOccurs="1" minOccurs="0" name="type" type="ITResource:ServiceType"/>
          <element maxOccurs="1" minOccurs="0" name="image" type="string"/>
          <element maxOccurs="1" minOccurs="0" name="flavor" type="string"/>
          <element maxOccurs="1" minOccurs="0" name="msplID" type="string"/>
          <element maxOccurs="1" minOccurs="0" name="domainID" type="string"/>
				</sequence>
          <attribute name="id" type="string"/>
	</complexType>	

	<simpleType name="ServiceType">
		<restriction base="string">
			<enumeration value="5GCore"/>
		</restriction>
	</simpleType>


		<complexType name="SSLALink">
				<sequence>
            <element maxOccurs="unbounded" minOccurs="0" name="sloID" type="string"/>
				</sequence>
          <attribute name="id" type="string"/>
	</complexType>	
	
		<complexType name="MSPLLink">
				<sequence>
            <element maxOccurs="unbounded" minOccurs="0" name="msplID" type="string"/>
				</sequence>
	</complexType>		
	
	    <complexType name="NEST">
				<sequence>
          <element maxOccurs="1" minOccurs="0" name="availability" type="float"/>
          <element maxOccurs="1" minOccurs="0" name="mmtel_support" type="int"/>
          <element maxOccurs="1" minOccurs="0" name="session_and_service_continuity_support" type="int"/>
          <element maxOccurs="1" minOccurs="0" name="three_gpp_5g_qos" type="int"/>
				</sequence>
	</complexType>	
	
	<complexType name="FiveGSecuritySliceAction">
		<complexContent>
		<extension base="ITResource:ConfigurationAction">
				<sequence>
					<element name="fiveGSecuritySliceActionType" type="ITResource:FiveGSecuritySliceActionType"/>
          <element maxOccurs="unbounded" minOccurs="1" name="securedService" type="ITResource:SecuredService"/>
          <element maxOccurs="1" minOccurs="0" name="nest" type="ITResource:NEST"/>
				</sequence>
				</extension>
		</complexContent>
	</complexType>

	<simpleType name="FiveGSecuritySliceActionType">
		<restriction base="string">
			<enumeration value="DEPLOY"/>
			<enumeration value="UPDATE"/>
			<enumeration value="REMOVE"/>
		</restriction>
	</simpleType>

<!--  INSPIRE 5G Secured Service Policy -->
		<complexType name="FiveGSecuredServiceAction">
		<complexContent>
			<extension base="ITResource:ConfigurationAction">
				<sequence>
					<element name="fiveGSecuredServiceActionType" type="ITResource:FiveGSecuritySliceActionType"/>
          <element name="securedService" type="ITResource:SecuredService"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	
	<complexType name="DataProtectionCondition">
		<complexContent>
			<extension base="ITResource:ConfigurationCondition">
				<sequence>
					<element maxOccurs="1" minOccurs="0" name="packetFilterCondition"
						type="ITResource:PacketFilterCondition"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="PurposeCondition">
		<complexContent>
			<extension base="ITResource:ConfigurationCondition">
				<sequence>
					<element name="PurposeCondition" type="ITResource:PurposeConditionType"
						maxOccurs="unbounded" minOccurs="0"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<!-- ANASTACIA Privacy -->
	<complexType name="PrivacyConfigurationCondition">
		<complexContent>
			<extension base="ITResource:FilteringConfigurationCondition">
				<sequence>
					<element name="Subject" type="string" minOccurs="0"/>
					<element name="Target" type="string" minOccurs="0"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="PrivacyAction">
		<complexContent>
			<extension base="ITResource:ConfigurationAction">
				<sequence>
					<element name="PrivacyActionType" type="ITResource:PrivacyActionType"
						minOccurs="1"/>
					<element name="PrivacyMethod" type="ITResource:PrivacyMethod" minOccurs="1"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="PrivacyMethod"> </complexType>

	<complexType name="PrivacyIBMethod">
		<complexContent>
			<extension base="ITResource:PrivacyMethod">
				<sequence>
					<element name="IB" type="string"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="PrivacyABMethod">
		<complexContent>
			<extension base="ITResource:PrivacyMethod">
				<sequence>
					<element name="attribute" type="ITResource:KeyValue" maxOccurs="unbounded"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="PrivacyPKIMethod">
		<complexContent>
			<extension base="ITResource:PrivacyMethod">
				<sequence>
					<element name="pkiParameters" type="ITResource:AuthenticationParameters"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>


	<complexType name="KeyValue">
		<sequence>
			<element name="key" type="string"/>
			<element name="value" type="string"/>
		</sequence>
	</complexType>


	<simpleType name="PrivacyActionType">
		<restriction base="string">
			<enumeration value="DATA"/>
		</restriction>
	</simpleType>

	<!-- ANASTACIA Data aggregation -->
	<complexType name="DataAgreggationConfigurationConditions">
		<complexContent>
			<extension base="ITResource:ConfigurationCondition">
				<sequence>
					<element maxOccurs="unbounded" minOccurs="1"
						name="dataAggregationConfigurationCondition"
						type="ITResource:DataAggregationConfigurationCondition"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="DataAggregationConfigurationCondition">
		<complexContent>
			<extension base="ITResource:FilteringConfigurationCondition"> </extension>
		</complexContent>
	</complexType>

	<complexType name="DataAggregationAction">
		<complexContent>
			<extension base="ITResource:ConfigurationAction">
				<sequence>
					<element maxOccurs="unbounded" name="dataAggregationActionType"
						type="ITResource:DataAggregationActionType"/>
					<element name="aditionalRuleParameters" type="ITResource:KeyValue" minOccurs="0"
						maxOccurs="unbounded"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<simpleType name="DataAggregationActionType">
		<restriction base="string">
			<enumeration value="SIMPLE_AGGREGATION"/>
		</restriction>
	</simpleType>

	<!-- ANASTACIA Anonymity -->

	<complexType name="AnonymityConfigurationCondition">
		<complexContent>
			<extension base="ITResource:FilteringConfigurationCondition"> </extension>
		</complexContent>
	</complexType>


	<complexType name="AnonymityAction">
		<complexContent>
			<extension base="ITResource:ConfigurationAction">
				<sequence>
					<element maxOccurs="unbounded" name="anonymityActionType"
						type="ITResource:AnonymityActionType"/>
					<element maxOccurs="1" minOccurs="0" name="anonymityTarget"
						type="ITResource:AnonymityConfigurationCondition"/>
					<element maxOccurs="1" minOccurs="0" name="anonymityTechnologyParameters"
						type="ITResource:AnonymityTechnologyParameter"/>
					<element name="aditionalAnonymityParameters" type="ITResource:KeyValue"
						minOccurs="0" maxOccurs="unbounded"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<simpleType name="AnonymityActionType">
		<restriction base="string">
			<enumeration value="SENDER"/>
			<enumeration value="RECEIVER"/>
			<enumeration value="COMMUNICATION"/>
		</restriction>
	</simpleType>

	<complexType name="AnonymityTechnologyParameter">
		<complexContent>
			<extension base="ITResource:TechnologySpecificParameters">
				<sequence> </sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="OnionRoutingTechnologyParameter">
		<complexContent>
			<extension base="ITResource:AnonymityTechnologyParameter">
